{
    "name": "searchpe",
    "description": "Consulta RUC, DNI - Peru",
    "keywords": ["peru", "rest"],
    "homepage": "https://github.com/surenperu/searchpe",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Pariona",
            "email": "lx7pary@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-gd": "*",
        "ext-openssl": "*",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "katzgrau/klogger": "^1.2",
        "giansalex/peru-consult": "^2.2",
        "tuupola/cors-middleware": "^0.5.2",
        "webonyx/graphql-php": "^0.11.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "Peru\\Api\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public public/index_dev.php",
        "test": "phpunit"
    }

}
